@model CandidateProfileSystem.Models.Technology

<h2>Add New Technology</h2>
<form asp-action="Create" method="post">
    <div class="form-group">
        <label>Technology Name</label>
        <input asp-for="TechnologyName" class="form-control" />
        <span asp-validation-for="TechnologyName" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
